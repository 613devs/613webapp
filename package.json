		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.1.0"
	},
	"type": "module",
	"dependencies": {
		"firebase": "^9.23.0",
		"sveltefire": "^0.4.3"
	}
}
