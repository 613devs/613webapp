		"@sveltejs/kit": "^1.27.4",
		"@tailwindcss/typography": "^0.5.10",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.4.23",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"firebase-tools": "^13.0.2",
		"postcss": "^8.4.32",
		"prettier": "^3.0.0",
